		int[] arr = {4, 5, 11, 44, 56, 92, 100}; // sorted Array
		int findNumber = 11;
		
	}
	
	// recursive function -- using Binary Search
	public static int findNumber(int num, int[] arr) {
		int index = 0;
		int left = arr[0];
		int right = arr[arr.length - 1];
		System.out.println(right);
		
		return index;
	
	}

}


// Time complexity is : O(logN)